{
	"manifest_version": 3,
	"name": "Chat GPT AI Toolkit",
	"version": "1.0.0",
	"description": "A Chrome extension to enhance productivity with AI tools.",
	"permissions": ["storage", "activeTab", "scripting"],
	"host_permissions": ["*://*/*"],
	"action": {
		"default_popup": "index.html",
		"default_icon": {
			"32": "assets/icons/icon32.png"
		}
	},
	"icons": {
		"32": "assets/icons/icon32.png"
	}
}
